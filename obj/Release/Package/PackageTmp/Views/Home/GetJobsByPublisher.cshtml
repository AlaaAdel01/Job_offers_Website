@model IEnumerable<JopOffere.Models.JobsViewModel>

@foreach (var gr in Model)
{
    <h3>@gr.JobTitle</h3>
    <table class="table table-bordered table-hover">
        <tr>
            <th>
                Applicant
            </th>
            <th>
                Job title
            </th>
            <th>
                Message
            </th>
            <th>
                Apply Date
            </th>
        </tr>

        @foreach (var item in gr.Items)
        {
    <tr>
        <td>
            @item.user.UserName
        </td>
        <td>
            @item.job.JopTitle
        </td>
        <td>
            @item.Message
        </td>
        <td>
            @item.ApplyDate
        </td>


    </tr>
        }
    </table>
}
